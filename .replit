modules = ["flutter", "dart-3.8"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["flutter"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flutter App"

[[workflows.workflow]]
name = "Flutter App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd invoice_history_app && flutter run -d linux --web-port 5000"

[workflows.workflow.metadata]
outputType = "console"
